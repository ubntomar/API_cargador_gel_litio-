# ESP32 API Configuration
DEBUG=False
HOST=0.0.0.0
PORT=8000

# Serial Configuration
SERIAL_PORT=/dev/ttyS5
SERIAL_BAUDRATE=9600
SERIAL_TIMEOUT=10

# Rate Limiting
MIN_COMMAND_INTERVAL=0.05
MAX_REQUESTS_PER_MINUTE=600

# Cache
CACHE_TTL=1
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/esp32_api.log
